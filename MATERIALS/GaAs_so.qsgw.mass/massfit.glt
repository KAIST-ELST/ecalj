set grid
set ylabel "mass/mass(electron)"

# Fitting region
set xrange [0.03: 0.08] 
 f(x) = a+ x**2*(b + x**2*c) 
 fit f(x) "temp" u ($5):(($8+$16)/2.0) via a,b, c
# Plotting region
set xrange [0.0: 0.12]
 plot \
 "Band005Syml004Spin1.mass" u ($5):($8) lt 5 pt  5 w lp ti "band= 17",\
 "Band006Syml004Spin1.mass" u ($5):($8) lt 6 pt  6 w lp ti "band= 18",\
 f(x) 
print 'mass=',f(0)
